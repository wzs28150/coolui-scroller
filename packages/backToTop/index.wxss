.backToTop {
  background-color   : rgba(0, 0, 0, 0.6);
  background-image   : url('data:image/png;base64, ');
  background-size    : 45rpx;
  background-repeat  : no-repeat;
  background-position: center;
  width              : 80rpx;
  height             : 80rpx;
  position           : fixed;
  bottom             : 40rpx;
  right              : 30rpx;
  border-radius      : 100%;
  z-index            : 5;
  opacity            : 0.6;
}