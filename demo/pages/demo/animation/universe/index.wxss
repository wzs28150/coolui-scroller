.demopage {
  background-color: #352b76;
}

.demopage .pannel-inner .scroller-pannel {
  background-color: #443687;
  border-top      : 5px solid #352b76;
  /* margin-top   : -5px; */
  box-sizing      : border-box;
}

.demopage .pannel-inner .scroller-pannel .title,
.demopage .pannel-inner .scroller-pannel .des,
.demopage .pannel-inner .scroller-pannel .downtip {
  color: #fff;
}

.demopage .pannel-inner .scroller-pannel .des,
.demopage .pannel-inner .scroller-pannel .downtip {
  opacity: 0.7;
}

.demopage .pannel-inner .scroller-pannel .downicon {
  opacity: 0.7;
}

.demopage .pannel-inner .scroller-pannel .downicon .line {
  background-color: #fff !important;
}

.demopage .universe-wapper {
  position: relative;
  height  : 150px;
}

.demopage .universe-wapper .universe-bg {
  position : absolute;
  width    : 100%;
  height   : 100%;
  left     : 50%;
  top      : 50%;
  transform: translate(-50%, -50%);
  opacity  : 0.2;
}

.demopage .universe-wapper .universe-bg::after {
  content         : "";
  display         : block;
  background-color: rgba(255, 255, 255, 0.1);
  width           : 300px;
  height          : 300px;
  border-radius   : 100%;
  position        : absolute;
  top             : -250px;
  left            : -115px;
  transition      : all 0.4s;
}

.demopage .universe-wapper .universe-bg::before {
  content         : "";
  display         : block;
  background-color: rgba(255, 255, 255, 0.1);
  width           : 300px;
  height          : 300px;
  border-radius   : 100%;
  position        : absolute;
  bottom          : -250px;
  right           : -115px;
  transition      : all 0.4s;
}

.demopage .universe-wapper .earth-group {
  width   : 170rpx;
  height  : 170rpx;
  position: absolute;
  left    : 50%;
  top     : 50%;
}

.demopage .universe-wapper .earth-group .earth {
  width           : 100rpx;
  height          : 100rpx;
  background-color: #19a1f6;
  border-radius   : 100%;
  position        : absolute;
  left            : 50%;
  top             : 50%;
  transform       : translate(-50%, -50%) scale(5);
  overflow        : hidden;
  background-image: url("https://test.wzs.pub/pic/universe/earth.jpg");
  background-size : 232rpx 100rpx;
  animation       : earth-play 3s infinite linear;
  z-index         : 100;
}

.demopage .universe-wapper .earth-group .earth::after {
  content            : "";
  display            : block;
  width              : 150rpx;
  height             : 150rpx;
  border-radius      : 100%;
  box-shadow         : 25px -10px 5px rgba(0, 0, 0, 0.3) inset;
  position           : absolute;
  left               : 50%;
  top                : 50%;
  transform          : translate(-50%, -50%);
  background-position: 50% 50%;
}

.demopage .universe-wapper .moon {
  width           : 50px;
  height          : 50px;
  background-color: #d5d3d7;
  position        : absolute;
  left            : 50%;
  top             : 50%;
  border-radius   : 50%;
  box-shadow      : 15px -10px 5px rgba(0, 0, 0, 0.3) inset;
  transform       : matrix3d(0.9, 0, 0, 0, 0, 0.9, 0, 0, 0, 0, 1, 0, 270, 0, 0, 1);
}

.demopage .universe-wapper .moon.loading {
  animation: rotate 3s linear infinite;
}

.demopage .universe-wapper .sun {
  width           : 100rpx;
  height          : 100rpx;
  border-radius   : 100%;
  background-color: #fec25b;
  position        : absolute;
  right           : 50rpx;
  top             : 10px;
  box-shadow      : 0 0 40px #fec25b;
}

.demopage .universe-wapper .jupiter {
  width           : 200rpx;
  height          : 200rpx;
  border-radius   : 100%;
  box-shadow      : 15px -10px 5px rgba(0, 0, 0, 0.3) inset;
  background-color: #f18a62;
  position        : absolute;
  left            : 50px;
  bottom          : -50rpx;
  opacity         : 0;
}

.demopage .universe-wapper .jupiter.loosen,
.demopage .universe-wapper .jupiter.loading {
  opacity: 1;
}

.demopage .universe-wapper .ball {
  width           : 20rpx;
  height          : 20rpx;
  border-radius   : 100%;
  background-color: #d5d3d7;
  box-shadow      : 2px -2px 5px rgba(0, 0, 0, 0.3) inset;
  position        : absolute;
  left            : 40%;
  top             : 20%;
}

@keyframes earth-play {
  to {
    background-position: 250% 50%;
  }
}

@keyframes gogogo {
  0% {
    transform: translate(-50%, -50%) rotateX(100deg) rotateY(-16deg) rotateZ(0deg);
  }

  100% {
    transform: translate(-50%, -50%) rotateX(100deg) rotateY(-16deg) rotateZ(360deg);
  }
}

@keyframes gogogo1 {
  0% {
    transform: rotateX(75deg) rotateY(0deg) rotateZ(180deg);
  }

  100% {
    transform: rotateX(75deg) rotateY(0deg) rotateZ(-180deg);
  }
}

